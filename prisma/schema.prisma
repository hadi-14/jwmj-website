model Form {
  id          String           @id @default(cuid()) @db.VarChar(255)
  name        String           @db.Char(255)
  description String?          @db.VarChar(500)
  formType    String           @db.VarChar(100)
  version     Int              @default(1)
  isActive    Boolean          @default(true)
  pdfFileUrl  String           @db.VarChar(500)
  pdfFileName String           @db.Char(255)
  createdAt   DateTime         @default(now()) @db.DateTime(0)
  updatedAt   DateTime         @default(now()) @updatedAt @db.DateTime(0)
  fields      FormField[]
  submissions FormSubmission[]

  @@index([formType], map: "formType")
  @@index([isActive], map: "isActive")
}

model FormField {
  id             String           @id @default(cuid()) @db.VarChar(255)
  formId         String           @db.VarChar(255)
  fieldName      String           @db.Char(255)
  fieldLabel     String           @db.Char(255)
  fieldType      String           @db.Char(50)
  fieldOrder     Int
  isRequired     Boolean          @default(false)
  isHidden       Boolean          @default(false)
  placeholder    String?          @db.Char(255)
  helpText       String?          @db.VarChar(500)
  defaultValue   String?          @db.VarChar(500)
  validationRule String?          @db.VarChar(1000)
  options        String?          @db.Text
  columnWidth    String           @default("full") @db.VarChar(50)
  createdAt      DateTime         @default(now()) @db.DateTime(0)
  updatedAt      DateTime         @default(now()) @updatedAt @db.DateTime(0)
  form           Form             @relation(fields: [formId], references: [id], onDelete: Cascade, map: "fk_formfield_form")
  fieldValues    FormFieldValue[]

  @@unique([formId, fieldName], map: "uq_formfield_name")
  @@index([formId], map: "formId")
}

model FormSubmission {
  id               String           @id @default(cuid()) @db.VarChar(255)
  formId           String           @db.VarChar(255)
  memberComputerId Decimal?         @db.Decimal(18, 0)
  submissionDate   DateTime         @default(now()) @db.DateTime(0)
  status           String           @default("draft") @db.Char(50)
  submittedBy      String?          @db.Char(255)
  approvedBy       String?          @db.Char(255)
  approvedDate     DateTime?        @db.DateTime(0)
  notes            String?          @db.VarChar(1000)
  ipAddress        String?          @db.Char(50)
  userAgent        String?          @db.VarChar(500)
  isDeleted        Boolean          @default(false)
  createdAt        DateTime         @default(now()) @db.DateTime(0)
  updatedAt        DateTime         @default(now()) @updatedAt @db.DateTime(0)
  auditLogs        FormAuditLog[]
  fieldValues      FormFieldValue[]
  form             Form             @relation(fields: [formId], references: [id], onDelete: Cascade, map: "fk_formsub_form")

  @@index([formId], map: "formId")
  @@index([memberComputerId], map: "memberComputerId")
  @@index([status], map: "status")
  @@index([submissionDate], map: "submissionDate")
}

model FormFieldValue {
  id           String         @id @default(cuid()) @db.VarChar(255)
  submissionId String         @db.VarChar(255)
  fieldId      String         @db.VarChar(255)
  value        String?        @db.Text
  fileUrl      String?        @db.VarChar(500)
  fileName     String?        @db.Char(255)
  fileSize     Int?
  createdAt    DateTime       @default(now()) @db.DateTime(0)
  updatedAt    DateTime       @default(now()) @updatedAt @db.DateTime(0)
  field        FormField      @relation(fields: [fieldId], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_ffv_field")
  submission   FormSubmission @relation(fields: [submissionId], references: [id], onDelete: Cascade, map: "fk_ffv_submission")

  @@unique([submissionId, fieldId], map: "uq_ffv")
  @@index([fieldId], map: "fieldId")
  @@index([submissionId], map: "submissionId")
}

model FormAuditLog {
  id           String         @id @default(cuid()) @db.VarChar(255)
