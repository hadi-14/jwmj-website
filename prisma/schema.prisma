  fieldValues       FormFieldValue[]
  
  @@unique([formId, fieldName])
  @@index([formId])
}

// Form Submissions - Main Data Table
model FormSubmission {
  id                String   @id @default(cuid())
  formId            String
  memberComputerId  Decimal? @db.Decimal(18, 0)
  submissionDate    DateTime @default(now())
  status            String   @default("draft") @db.NVarChar(50)
  submittedBy       String?  @db.NVarChar(255)
  approvedBy        String?  @db.NVarChar(255)
  approvedDate      DateTime?
  notes             String?  @db.NVarChar(1000)
  ipAddress         String?  @db.NVarChar(50)
  userAgent         String?  @db.NVarChar(500)
  isDeleted         Boolean  @default(false)
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
  
  form              Form     @relation(fields: [formId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  fieldValues       FormFieldValue[]
  auditLogs         FormAuditLog[]
  
  @@index([formId])
  @@index([status])
  @@index([memberComputerId])
  @@index([submissionDate])
}

// Form Field Values - Stores actual form data
// FIX: Changed to use NoAction to prevent cyclic cascade
model FormFieldValue {
  id                String   @id @default(cuid())
  submissionId      String
  fieldId           String
  value             String?  @db.NVarChar(max)
  fileUrl           String?  @db.NVarChar(500)
  fileName          String?  @db.NVarChar(255)
  fileSize          Int?
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
  
  submission        FormSubmission @relation(fields: [submissionId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  field             FormField      @relation(fields: [fieldId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  
  @@unique([submissionId, fieldId])
  @@index([submissionId])
  @@index([fieldId])
}

// Form Metadata/Audit Log
model FormAuditLog {
  id                String   @id @default(cuid())
  submissionId      String
  action            String   @db.NVarChar(100)
  changedBy         String   @db.NVarChar(255)
  changes           String?  @db.NVarChar(max)
  timestamp         DateTime @default(now())
  ipAddress         String?  @db.NVarChar(50)
  
  submission        FormSubmission @relation(fields: [submissionId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  
  @@index([submissionId])
  @@index([timestamp])
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  name      String?
  password  String
  role      String   @default("USER") @db.NVarChar(20)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}